<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragDis_log_cab_request_details">

    <EditText
        android:id="@+id/cusEmailLCD"
        android:layout_width="320dp"
        android:layout_height="46dp"
        app:layout_constraintHorizontal_bias="0.681"
        android:ems="10"
        android:hint="Customer Email"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:background="@drawable/etcustom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.123" />

    <EditText
        android:id="@+id/tpCRD"
        android:layout_width="320dp"
        android:layout_height="46dp"
        app:layout_constraintHorizontal_bias="0.681"
        android:ems="10"
        android:hint="Customer phone"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:background="@drawable/etcustom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.218" />

    <EditText
        android:id="@+id/locationCRD"
        android:layout_width="320dp"
        android:layout_height="46dp"
        app:layout_constraintHorizontal_bias="0.681"
        android:ems="10"
        android:hint="location"
        android:inputType="text"
        android:paddingLeft="10dp"
        android:background="@drawable/etcustom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.321" />

    <EditText
        android:id="@+id/desentinationCRD"
        android:layout_width="320dp"
        android:layout_height="46dp"
        app:layout_constraintHorizontal_bias="0.681"
        android:ems="10"
        android:hint="Destination"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:background="@drawable/etcustom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.421" />

    <EditText
        android:id="@+id/driverNameCRD"
        android:layout_width="320dp"
        android:layout_height="46dp"
        app:layout_constraintHorizontal_bias="0.681"
        android:ems="10"
        android:hint="Driver Name"
        android:paddingLeft="10dp"
        android:background="@drawable/etcustom"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.532" />

    <EditText
        android:id="@+id/customerNameCRD"
        android:layout_width="320dp"
        android:layout_height="46dp"
        app:layout_constraintHorizontal_bias="0.681"
        android:ems="10"
        android:hint="Customer Name"
        android:paddingLeft="10dp"
        android:background="@drawable/etcustom"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.637" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LOG CAB REQUEST DETAILS"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />

    <Button
        android:id="@+id/LogCRDbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log"
        android:ems="10"
        android:padding="12dp"
        android:textSize="15dp"
        app:cornerRadius="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.734" />

</androidx.constraintlayout.widget.ConstraintLayout>